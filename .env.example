# .env.example

# Supabase PostgreSQL connection string (using psycopg v3 driver)
# Format: postgresql+psycopg://postgres.[PROJECT-REF]:[PASSWORD]@aws-0-[REGION].pooler.supabase.com:6543/postgres
SQLALCHEMY_DATABASE_URI=postgresql+psycopg://postgres.[PROJECT-REF]:[PASSWORD]@aws-0-[REGION].pooler.supabase.com:6543/postgres

SECRET_KEY=<your-secret-key>
